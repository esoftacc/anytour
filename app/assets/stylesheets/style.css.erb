	/*1st sub level menu*/
	.cocaMenu ul li ul{
		position: absolute;
		left: 0;
		display: none;
		/*collapse all sub menus to begin with*/
		visibility: hidden;z-index: 100;
		top:54px !important;
		background: rgba(255,255,255,0.8);
		border-radius:0 0 3px 3px;
		padding:4px 0 12px !important;
